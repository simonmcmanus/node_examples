{
    "name": "Bookmark",
    "description": "Adds the current page to my bookmarking system.",
    "version": "1.0",
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ],
    "browser_action": {
        "default_title": "Bookmark This Page",
        "default_icon": "icon.png",
        "popup": "popup.html"
    }
}